#include "AmberReflection.hpp"

AMBER_REFLECT
{
	ARefl::RegisterCompound<NS::MyBaseTemplateClass>()
		.MemberField(NS::MyBaseTemplateClass, field1, { new AProperty() });

	ARefl::RegisterCompound<NS::MyOtherTemplatedClass>().Base(MyBaseTemplateClass<class NS::TemplateArg>)
		.MemberField(NS::MyOtherTemplatedClass, field2, { new AProperty() });
}